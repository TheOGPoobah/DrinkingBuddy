<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true" >
        <shape>
            <solid
                android:color="#52c234" />
            <stroke
                android:width="1dp"
                android:color="#061700" />
            <corners
                android:radius="1dp" />

        </shape>
    </item>
    <item>
        <shape>
            <gradient
                android:startColor="#52c234"
                android:endColor="#061700"
                android:angle="270" />
            <stroke
                android:width="1dp"
                android:color="#061700" />
            <corners
                android:radius="1dp" />

        </shape>
    </item>
</selector>